    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "~7.7.4",
    "@material-ui/core": "~4.7.1",
    "@material-ui/icons": "~4.5.1",
    "@material-ui/styles": "~4.7.1",
    "@storybook/addon-a11y": "~5.2.8",
    "@storybook/addon-actions": "~5.2.8",
    "@storybook/addon-centered": "~5.2.8",
    "@storybook/addon-info": "~5.2.8",
