    "@testing-library/react": "~9.3.2",
    "@types/classnames": "~2.2.9",
    "@types/enzyme": "~3.10.3",
    "@types/jest": "~24.0.18",
    "@types/lodash": "~4.14.149",
    "@types/node": "~12.12.11",
    "@types/react": "~16.9.14",
    "@types/react-dom": "~16.9.4",
    "@types/react-test-renderer": "~16.9.1",
    "@typescript-eslint/eslint-plugin": "~2.10.0",
    "@typescript-eslint/parser": "~2.10.0",
